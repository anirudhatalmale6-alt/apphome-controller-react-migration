# =============================================
# Environment Configuration for AppHomeController React Migration
# Copy this file to .env and fill in the values
# =============================================

# Backend API Gateway URL
# This is the URL where your AngularJS backend server is running
# Example: http://localhost:8090/onebase
VITE_API_GATEWAY=http://localhost:8090/onebase

# Application Mode
VITE_APP_MODE=development

# Enable/Disable Encryption (for debugging)
VITE_ENABLE_ENCRYPTION=true

# Session timeout in minutes
VITE_SESSION_TIMEOUT=30

# MFA Settings
VITE_MFA_ENABLED=true
